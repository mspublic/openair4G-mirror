OPENAIR2_DIR=../../
rmmod nasmesh
insmod $OPENAIR2_DIR/NAS/DRIVER/MESH/nasmesh.ko


# UE
ifconfig nasmesh0 10.0.1.3 netmask 255.255.255.0 broadcast 10.0.1.255

$OPENAIR2_DIR/NAS/DRIVER/MESH/RB_TOOL/rb_tool -a -c0 -i0 -z0 -s 10.0.1.3 -t 10.0.1.1 -r 4 

cd $OPENAIR2_DIR/SIMULATION/USER_TOOLS/LAYER2_SIM
./mac_sim 2

