# Define Device, Package, And Speed Grade
CONFIG PART = XC5VLX110T-FF1136-1 ;

#####################################################################################################################################################
#####################################################################################################################################################
####                                                                                                                                             ####
####                                       I / O   P I N   L O C A T I O N   C O N S T R A I N T S                                               ####
####                                                                                                                                             ####
#####################################################################################################################################################
#####################################################################################################################################################

       #    Name of the            #  Location           #    Name of           # Name of the          # ... and its   #  Location     # Name of the
       #    signal port            # pin number          #  the corres-         # remote item          #  reference    # pin number    # signal port
       # in the Top-Level          #   on the            # ponding signal       #  connected           #  number on    #   on the      #   on the
       #    RTL design             #    FPGA             #  on the PCB          # on the PCB...        #  the board    # remote item   # remote item

#####################################################################################################################################################
# Bank 3
#####################################################################################################################################################
NET    "clk"                       LOC = "J16"   ;       # USRCLK               # OSC_DUAL             # X6            # 8             # OUT (through R101)
#####################################################################################################################################################
# Bank 19
#####################################################################################################################################################
# USER RESET
NET    "resetn"                    LOC = "G27"   ;       # USR_RESET            # TL1105F100Q          # SW5           # 1             # p1
# LEDs
NET    "errorn"                    LOC = "F26"   ;       # led 7: IU error mode
NET    "dsuact"                    LOC = "F25"   ;       # led 6: IU debug mode
# GRGPIO (leds 5 downto 0)
NET    "gpio(5)"                   LOC = "H24"   ;       # led 5: GPIO bit 5
NET    "gpio(4)"                   LOC = "H25"   ;       # led 4: GPIO bit 4
NET    "gpio(3)"                   LOC = "G26"   ;       # led 3: GPIO bit 3
NET    "gpio(2)"                   LOC = "G25"   ;       # led 2: GPIO bit 2
NET    "gpio(1)"                   LOC = "J26"   ;       # led 1: GPIO bit 1
NET    "gpio(0)"                   LOC = "J27"   ;       # led 0: GPIO bit 0

#####################################################################################################################################################
#####################################################################################################################################################
##                                                                                                                                                 ##
##                                                     T I M I N G   c o n s t r a i n t s                                                         ##
##                                                                                                                                                 ##
#####################################################################################################################################################
#####################################################################################################################################################

#####################################################################################################################################################
#                                              M a i n   C l o c k      &      I  /  O    d e l a y s                                               #
#####################################################################################################################################################
NET clk PERIOD = 10 ns ;
OFFSET =   OUT   :   5.000   :   AFTER  clk   ;
OFFSET =   IN    :   5.000   :   BEFORE clk   ;

#####################################################################################################################################################
#####################################################################################################################################################
####                                                                                                                                             ####
####                                           P U L L - U P / P U L L - D O W N   c o n s t r a i n t s                                         ####
####                                                                                                                                             ####
#####################################################################################################################################################
#####################################################################################################################################################
NET    "resetn"                      PULLUP      ;

#####################################################################################################################################################
#####################################################################################################################################################
####                                                                                                                                             ####
####                                                    P R O H I B I T   n o n   u s e d   p i n s                                              ####
####                                                                                                                                             ####
