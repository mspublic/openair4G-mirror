    bool 'Enable Wireless Modem features   '  CONFIG_CMIMO1_ENABLE
    if [ "$CONFIG_CMIMO1_ENABLE" = "y" ]; then
      hex 'Carbus-MIMO-1 I/O area start address (haddr[19:8])   '   CONFIG_CMIMO1_IO 200
      hex 'Register file for RF control I/O area offset (haddr[7:0])   '   CONFIG_CMIMO1_RFCTRL_IO_OFFSET 00
      hex 'Register file for ADAC control I/O area offset (haddr[7:0])   '   CONFIG_CMIMO1_ADAC_IO_OFFSET 20
      hex 'Register file for Widens SPI control/config I/O area offset (haddr[7:0])   '   CONFIG_CMIMO1_SPI_IO_OFFSET 40
      hex 'Register file for Idromel SPI control/config I/O area offset (haddr[7:0])   '   CONFIG_CMIMO1_IDROMEL_SPI_IO_OFFSET 80
      hex 'Carbus-MIMO-1 Memory area start address (haddr[31:20])  '  CONFIG_CMIMO1_HADDR C00
      hex 'Carbus-MIMO-1 Memory area mask address (haddr[31:20])  '  CONFIG_CMIMO1_HMASK FFF
      hex 'ADC0 Memory bank area offset (haddr[19:16])   '   CONFIG_CMIMO1_ADAC_ADC0_HOFFSET 0
      choice 'ADC0 Memory bank size' \
      "2   CONFIG_CMIMO1_ADAC_RAMADC0_KBSZ_2 \
       4   CONFIG_CMIMO1_ADAC_RAMADC0_KBSZ_4 \
       8   CONFIG_CMIMO1_ADAC_RAMADC0_KBSZ_8 \
       16  CONFIG_CMIMO1_ADAC_RAMADC0_KBSZ_16 \
       32  CONFIG_CMIMO1_ADAC_RAMADC0_KBSZ_32 \
       64  CONFIG_CMIMO1_ADAC_RAMADC0_KBSZ_64" 2
      hex 'ADC1 Memory bank area offset (haddr[19:16])   '   CONFIG_CMIMO1_ADAC_ADC1_HOFFSET 1
      choice 'ADC1 Memory bank size' \
      "2   CONFIG_CMIMO1_ADAC_RAMADC1_KBSZ_2 \
       4   CONFIG_CMIMO1_ADAC_RAMADC1_KBSZ_4 \
       8   CONFIG_CMIMO1_ADAC_RAMADC1_KBSZ_8 \
       16  CONFIG_CMIMO1_ADAC_RAMADC1_KBSZ_16 \
       32  CONFIG_CMIMO1_ADAC_RAMADC1_KBSZ_32 \
       64  CONFIG_CMIMO1_ADAC_RAMADC1_KBSZ_64" 2
      hex 'DAC0 Memory bank area offset (haddr[19:16])   '   CONFIG_CMIMO1_ADAC_DAC0_HOFFSET 2
      choice 'DAC0 Memory bank size' \
      "2   CONFIG_CMIMO1_ADAC_RAMDAC0_KBSZ_2 \
       4   CONFIG_CMIMO1_ADAC_RAMDAC0_KBSZ_4 \
       8   CONFIG_CMIMO1_ADAC_RAMDAC0_KBSZ_8 \
       16  CONFIG_CMIMO1_ADAC_RAMDAC0_KBSZ_16 \
       32  CONFIG_CMIMO1_ADAC_RAMDAC0_KBSZ_32 \
       64  CONFIG_CMIMO1_ADAC_RAMDAC0_KBSZ_64" 4
      hex 'DAC1 Memory bank area offset (haddr[19:16])   '   CONFIG_CMIMO1_ADAC_DAC1_HOFFSET 3
      choice 'DAC1 Memory bank size' \
      "2   CONFIG_CMIMO1_ADAC_RAMDAC1_KBSZ_2 \
       4   CONFIG_CMIMO1_ADAC_RAMDAC1_KBSZ_4 \
       8   CONFIG_CMIMO1_ADAC_RAMDAC1_KBSZ_8 \
       16  CONFIG_CMIMO1_ADAC_RAMDAC1_KBSZ_16 \
       32  CONFIG_CMIMO1_ADAC_RAMDAC1_KBSZ_32 \
       64  CONFIG_CMIMO1_ADAC_RAMDAC1_KBSZ_64" 4
      int 'Irq number of Carbus-MIMO-1 feature'   CONFIG_CMIMO1_IRQ 6
    fi
