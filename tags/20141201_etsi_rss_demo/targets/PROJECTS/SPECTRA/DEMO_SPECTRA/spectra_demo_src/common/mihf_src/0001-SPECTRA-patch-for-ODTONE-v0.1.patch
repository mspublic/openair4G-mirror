+    int index = 0;
+
+    index += std::sprintf(&buffer[index], "plmn: -%hhx--%hhx--%hhx-\n", addr.plmn_id[0], addr.plmn_id[1], addr.plmn_id[2]);
+    index += std::sprintf(&buffer[index], "cell_id: %hhx\n", addr._cell_id);
+    return buffer;
+}
