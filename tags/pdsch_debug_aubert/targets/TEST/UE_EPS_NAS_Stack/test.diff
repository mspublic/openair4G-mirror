diff -rNu Final_pkg_UEStack/Test/main.c Final_pkg_UEStack.barisd/Test/main.c
--- Final_pkg_UEStack/Test/main.c	1970-01-01 01:00:00.000000000 +0100
+++ Final_pkg_UEStack.barisd/Test/main.c	2012-02-07 17:07:51.000000000 +0100
@@ -0,0 +1,23 @@
+
+#include <stdio.h>
+#include "../Framework/hdr/process.h"
+
+extern void Start_Ue(gchar** argv);
+
+int main(int argc, char** argv)
+{
+	int i = 0;
+
+	printf("Commandline arguments:\n");
+	for (; i < argc; i++) {
+		printf("%d. argument: '%s'\n", i, argv[i]);
+	}
+
+	/*
+	 * Start UE
+	 */
+	Start_Ue(argv);
+
+	return 0;
+}
+
diff -rNu Final_pkg_UEStack/Test/Makefile Final_pkg_UEStack.barisd/Test/Makefile
--- Final_pkg_UEStack/Test/Makefile	1970-01-01 01:00:00.000000000 +0100
+++ Final_pkg_UEStack.barisd/Test/Makefile	2012-02-08 16:30:43.000000000 +0100
@@ -0,0 +1,36 @@
+
+CC = gcc
+
+INCLUDE = -pthread -I/usr/local/include/gnet-2.0 \
+          -I/usr/local/lib/gnet-2.0/include \
+          -I/usr/local/include/glib-2.0 \
+          -I/usr/lib/glib-2.0/include \
+          -I/usr/lib/gnet-2.0/include \
+          -I/usr/include/gnet-2.0 \
+          -I/usr/local/lib/glib-2.0/included \
+          -I/usr/local/include/gnet-2.0.so \
+          -I/usr/local/lib/gnet-2.0/include \
+          -I/usr/local/include/glib-2.0 \
+          -I/usr/local/lib/glib-2.0/include \
+
+LIB = -pthread -L/usr/local/lib \
+	-lgnet-2.0 \
+	-lgthread-2.0 \
+	-lrt \
+	-lglib-2.0 
+
+CFLAGS = -g -I ../UEStack/eMM/main/hdr -I ../UEStack/eMM/msg/hdr/ \
+	 -I ../Framework/hdr/ -I ../UEStack/interfaceModule/hdr/  \
+	 -I ../UEStack/timerMgmt/hdr -I ../UEStack/eSM/main/hdr/  \
+	 -I ../UEStack/eSM/msg/hdr/ ${LIB}
+
+tester: main.o
+	${CC} ${CFLAGS} -o $@ main.o \
+	../Framework/build/Framework.a ../UEStack/eMM/msg/build/libMessaging.so
+
+main.o: main.c ../UEStack/eMM/main/hdr/emmContext.h ../Framework/build/*.o
+	${CC} ${INCLUDE} ${CFLAGS} -c main.c
+
+clean:
+	rm -f *.o tester
+
