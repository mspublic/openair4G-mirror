killall mac_sim
OPENAIR2_DIR=/homes/anouar/openair2
cd $OPENAIR2_DIR
sudo rmmod nasmesh
sudo insmod NAS/DRIVER/MESH/nasmesh.ko


# UE 2
sudo ifconfig nasmesh0 10.0.1.3 netmask 255.255.255.0 broadcast 10.0.1.255

NAS/DRIVER/MESH/RB_TOOL/rb_tool -a -c0 -i0 -z0 -s 10.0.1.3 -t 10.0.1.1 -r 4 



cd SIMULATION/USER_TOOLS/LAYER2_SIM
./mac_sim -m2 -t ../../TOPOLOGIES/eth_3nodes.top
